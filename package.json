{
  "name": "parcel-project-template",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "homepage": "https://windyboss.github.io/goit-js-hw-11",
  "scripts": {
    "dev": "parcel src/*.html",
    "build": "parcel build src/*.html --public-url /goit-js-hw-11/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windyboss/goit-js-hw-11.git"
  },
  "keywords": [],
  "author": "Vitaliy Zhyryk <zhyryk1997@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goitacademy/parcel-project-template/issues"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-handlebars-precompile": "^1.0.2",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "posthtml-include": "^1.6.2",
    "sass": "^1.32.8"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "infinite-scroll": "^4.0.1",
    "notiflix": "^3.2.2",
    "simplelightbox": "^2.10.1"
  },

  "customDistStructure": {
    "config": {
      ".js": "js",
      ".js.map": "js",
      "images": [
        ".jpg",
        ".png"
      ],
      ".css": "css",
      ".css.map": "css",
      ".fileExtension": "folder/in/dist",
      "folder/in/dist": [ ".file", ".extensions" ]
    },
    "options": {
      "development": true
    }
  }
}
